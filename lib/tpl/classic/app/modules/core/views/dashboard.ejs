<div class="dashboard container">
  <div class="row">
    <div class="col-md-8 offset-md-2">
      <h1>Dashboard</h1>
      <div class="card">
        <div class="card-body">
          <div class="row">
            <div class="col-md-3 text-center">
              <i class="fa fa-user text-muted" style="font-size: 70px;"></i>
            </div>
            <div class="col-md-9">
              <p>
                <%=user.firstName%> <%=user.lastName%>
                <br />
                <%=user.email%>
              </p>
              <p>
                <a href="/my-profile">My Profile</a>
              </p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
